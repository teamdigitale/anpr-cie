					<xs:documentation>
						<p>Stato civile</p>
						<p>
							<b>VALORI AMMESSI</b>
							<ol>
								<li>
									Valori presenti nella
									<a
										href="https://italia.github.io/anpr/tab/tab_stato_civile.html.html"
										target="_self">tabella 1</a>
								</li>
							</ol>
						</p>
						<p>
							<b>REGOLE / CONDIZIONI</b>
							<ol>
								<li>Verifica codice</li>
							</ol>
						</p>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="noteStatoCivile" type="charMax240"
				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						<p>Nota stato civile</p>
						<p>
							<b>REGOLE / CONDIZIONI</b>
							<ol>
								<li>E' valorizzato solo nel caso in cui lo Stato Civile sia 9 -
									Non classificabile / ignoto / non documentato
								</li>
							</ol>
						</p>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="statoCivileND" type="tipoSN" minOccurs="0"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						<p>Stato civile non documentato</p>
						<p>
							<b>REGOLE / CONDIZIONI</b>
							<ol>
								<li>Se S stato civile non documentato</li>
							</ol>
						</p>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
