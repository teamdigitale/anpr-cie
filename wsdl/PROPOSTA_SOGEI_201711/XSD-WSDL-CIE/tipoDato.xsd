	<!-- INIZIO TIPI NUMERICI -->
	<!-- tipi numerici a lunghezza fissa -->
	<xs:simpleType name="numerico">
		<xs:restriction base="xs:string">
			<xs:whiteSpace value="collapse" />
			<xs:pattern value="(\d)+" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="numerico1">
		<xs:restriction base="numerico">
			<xs:length value="1" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="numerico3">
		<xs:restriction base="numerico">
			<xs:length value="3" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="numerico4">
		<xs:restriction base="numerico">
			<xs:length value="4" />
		</xs:restriction>
	</xs:simpleType>
	<!-- fine tipi numerici a lunghezza fissa -->

	<!-- tipi numerici a lunghezza massima -->
	<xs:simpleType name="numericoMax2">
		<xs:restriction base="numerico">
