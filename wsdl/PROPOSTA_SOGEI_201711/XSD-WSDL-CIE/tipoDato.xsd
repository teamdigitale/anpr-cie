		<xs:sequence>
			<xs:element name="tipoIndirizzo" type="tipoDatoIndirizzo"
				minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						<p>Tipo indirizzo</p>
						<p>
							<b>VALORI AMMESSI</b>
							<ol>
								<li>
									Valori presenti nella
									<a href="https://italia.github.io/anpr/tab/tab_tipo_indirizzo.html"
										target="_self">tabella 25</a>
								</li>
							</ol>
						</p>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="noteIndirizzo" type="charMax240"
				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						<p>Note sull'indirizzo</p>
						<p>
							<b>REGOLE / CONDIZIONI</b>
							<ol>
								<li>Obbligatorio se tipoIndirizzo = 9 - altro</li>
							</ol>
						</p>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice>
				<xs:element name="indirizzo" type="tipoIndirizzo">
					<xs:annotation>
						<xs:documentation>
							<p></p>
							<p>
								<b>REGOLE / CONDIZIONI</b>
								<ol>
									<li>In alternativa a localitaEstera per tipoIndirizzo diverso
										da 4 e 6
									</li>
								</ol>
							</p>
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="localitaEstera" type="tipoLocalitaEstera">
					<xs:annotation>
						<xs:documentation>
							<p></p>
							<p>
								<b>REGOLE / CONDIZIONI</b>
								<ol>
									<li>In alternativa a indirizzo per tipoIndirizzo uguale a 4 o 6
									</li>
								</ol>
							</p>
						</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:element name="presso" type="charMax100" minOccurs="0"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						<p>Indica la persona o la struttura presso la quale il soggetto è
							temporaneamente domiciliato
						</p>
						<p>
							<b>REGOLE / CONDIZIONI</b>
							<ol>
								<li>Per tipoIndirizzo uguale a 5 o 6</li>
							</ol>
						</p>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dataDecorrenzaResidenza" type="xs:date"
				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						<p>Data di decorezza della residenza</p>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
