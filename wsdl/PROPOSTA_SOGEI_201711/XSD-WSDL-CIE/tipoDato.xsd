	<!-- tipoGeneralitaPerRicerca -->
	<xs:complexType name="tipoGeneralitaPerRicerca">
		<xs:annotation>
			<xs:documentation>Contiene le generalità di un soggetto utilizzate in
				fase di ricerca
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="codiceFiscale" type="tipoDominioCodiceFiscale"
				minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						<p>Codice fiscale del soggetto</p>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice>
				<xs:element name="cognome" type="alfabeticoNomiCognomi">
					<xs:annotation>
						<xs:documentation>Il cognome del soggetto</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="senzaCognome" type="tipoSenza">
					<xs:annotation>
						<xs:documentation>Indica se il soggetto è senza cognome. Se
							presente deve assumere il valore 1
						</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:choice>
				<xs:element name="nome" type="alfabeticoNomiCognomi">
					<xs:annotation>
						<xs:documentation>Il nome del soggetto</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="senzaNome" type="tipoSenza">
					<xs:annotation>
						<xs:documentation>Indica se il soggetto è senza nome. Se presente
							deve assumere il valore 1
						</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:element name="sesso" type="tipoSesso" minOccurs="0"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Può assumere i seguenti valori: F, M, N (non
						conosciuto)
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dataNascita" type="xs:date" minOccurs="0"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation>La data di nascita del soggetto. Se è senza
						giorno indicare AAAAMM01, se è senza mese e giorno, indicare
						AAAA0101
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="senzaGiorno" type="tipoSenza" minOccurs="0"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Indica se data nascita è composta dal solo mese e
						anno se impostato deve valere 1
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="senzaGiornoMese" type="tipoSenza"
				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Indica se data nascita è composta dal solo anno
						se impostato deve valere 1
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="luogoNascita" type="tipoLuogoEvento"
				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Il luogo di nascita del soggetto
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="idSchedaSoggettoANPR" type="numericoMax15"
				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>identificativo della scheda soggetto attribuito
						da ANPR
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="codiceConsolato" type="charMax7"
				minOccurs="0" maxOccurs="1" />
		</xs:sequence>
	</xs:complexType>
