	<!-- fine tipi numerici a lunghezza massima -->

	<!-- ###################################################### FINE DATI ELEMENTARI 
		############################################################## -->

	<!-- ###################################################### INIZIO OGGETTI 
		CONDIVISI ############################################################## -->

	<!-- USATO IN TUTTI I MSG DI RICIHIESTA -->
	<!-- tipoDatoTestataRichiesta -->
	<xs:complexType name="tipoDatoTestataRichiesta">
		<xs:sequence>
			<xs:element name="idOperazioneComune" type="charMax30"
				minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						<p>Identificativo univoco attribuito all'operazione</p>
						<p>
							<b>REGOLE / CONDIZIONI</b>
							<ol>
								<li>Numerico crescente</li>
								<li>Se esiste in ANPR, il comune riceve come esito la risposta
									in precedenza fornita da ANPR con lo stesso ID
								</li>
								<li>Se non esiste in ANPR e inferiore all'ultimo inviato,
									l'elaborazione termina con errore
								</li>
							</ol>
						</p>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="codMittente" type="char6" minOccurs="1"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						<p>Il codice ISTAT del comune che effettua la richiesta</p>
						<p>
							<b>VALORI AMMESSI</b>
							<ol>
								<li>
									Valori presenti nella
									<a href="https://italia.github.io/anpr/tab/tab_comuni.html"
										target="_self">tabella 3</a>
								</li>
							</ol>
						</p>
						<p>
							<b>REGOLE / CONDIZIONI</b>
							<ol>
								<li>Verifica esistenza codice nella tabella 3</li>
							</ol>
						</p>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="codDestinatario" type="char6" minOccurs="1"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						<p>Il codice del destinatario</p>
						<p>
							<b>VALORI AMMESSI</b>
							<ol>
								<li>ANPR00</li>
							</ol>
						</p>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="operazioneRichiesta" type="tipoOperazioneRichiesta"
				minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						<p>Identificativo operazione richiesta</p>
						<p>
							<b>VALORI AMMESSI</b>
							<ol>
								<li>CIE2, Interrogazione Dati Anagrafici</li>
								<li>CIE8, Comunicazione Emissione</li>
								<li>CIE7, Comunicazione Annullamento</li>
							</ol>
						</p>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dataOraRichiesta" type="xs:dateTime"
				minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						<p>La data e ora in cui si invia la comunicazione anagrafica ad
							ANPR
						</p>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="tipoOperazione" type="tipoRichiedenteOperazione"
				minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						<p>Tipo del richiedente operazione</p>
						<p>
							<b>VALORI AMMESSI</b>
							<ol>
								<li>C, richiesta del cittadino</li>
							</ol>
						</p>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="tipoInvio" type="tipoInvioAmmesso"
				minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						<p>Specifica in quale ambiente si esegue il servizio</p>
						<p>
							<b>VALORI AMMESSI</b>
							<ol>
								<li>TEST</li>
								<li>PRE_SUB</li>
								<li>PROD</li>
							</ol>
						</p>
						<p>
							<b>REGOLE / CONDIZIONI</b>
							<ol>
								<li>Si verifica se il parametro è compatibile con l'ambiente di
									riferimento; se non compatibile il servizio termina con errore
								</li>
							</ol>
						</p>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="nomeApplicativo" type="charMax80"
				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						<p>Nome dell'applicativo utilizzato per l'invio</p>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="versioneApplicativo" type="charMax80"
				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						<p>Versione dell'applicativo utilizzato per l'invio</p>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fornitoreApplicativo" type="charMax80"
				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						<p>Fornitore del software</p>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="versioneServizio" type="charMax80"
				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						<p>Versione del servizio</p>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- fine tipoDatoTestataRichiesta -->

