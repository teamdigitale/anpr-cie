<?xml version="1.0" encoding="UTF-8"?>

<xs:schema xmlns="http://sogei.it/ANPR/tipoDato" xmlns:xs="http://www.w3.org/2001/XMLSchema"
	targetNamespace="http://sogei.it/ANPR/tipoDato" elementFormDefault="unqualified"
	attributeFormDefault="unqualified">

	<!-- ###################################################### INIZIO DATI 
		COMPLESSI ############################################################## -->
	<!-- alfanumericoComune massimo 80 caratteri -->
	<xs:simpleType name="alfanumericoComune">
		<xs:restriction base="xs:string">
			<xs:minLength value="1" />
			<xs:maxLength value="80" />
			<xs:pattern value="([À-ža-zA-Z\- &apos;/.,])*" />
		</xs:restriction>
	</xs:simpleType>
	<!-- Fine alfanumericoComune -->

	<!-- tipo alfabetico della provincia -->
	<xs:simpleType name="alfabeticoProvincia">
		<xs:restriction base="xs:string">
			<xs:length value="2" />
			<xs:pattern value="([À-ža-zA-Z\- &apos;,.])*" />
		</xs:restriction>
	</xs:simpleType>
	<!-- Fine tipo alfabetico della provincia -->
	<!-- tipo alfabetico dei nomi e cognomi -->
	<xs:simpleType name="alfabeticoNomiCognomi">
		<xs:restriction base="xs:string">
			<xs:whiteSpace value="preserve" />
			<xs:minLength value="1" />
			<xs:maxLength value="80" />
			<xs:pattern value="([À-ža-zA-Z\- &apos;,.])*" />
		</xs:restriction>
	</xs:simpleType>
	<!-- Fine tipo alfabetico dei nomi e cognomi -->
	<!-- tipo alfabetico senza nome o cognome -->
	<xs:simpleType name="tipoSenza">
		<xs:restriction base="numerico1">
			<xs:pattern value="1" />
		</xs:restriction>
	</xs:simpleType>
	<!-- Fine tipo alfabetico senza nome o cognome -->
	<!-- tipoDominioCodiceFiscale -->
	<xs:simpleType name="tipoDominioCodiceFiscale">
		<xs:restriction base="xs:string">
			<xs:minLength value="11" />
			<xs:maxLength value="16" />
			<!-- xs:pattern value="([A-Z0-9])*"/ -->
			<xs:pattern
				value="([A-Za-z]{6}[0-9]{2}[ABCDEHLMPRSTabcdehlmprst][0-9]{2}[A-Za-z][0-9]{3}[A-Za-z])|[0-9]{11}" />
		</xs:restriction>
	</xs:simpleType>
	<!-- Fine tipoDominioCodiceFiscale -->
	<!-- tipoSesso -->
	<xs:simpleType name="tipoSesso">
		<xs:restriction base="xs:string">
			<xs:pattern value="M|F|N|m|f|n" />
		</xs:restriction>
	</xs:simpleType>
	<!-- Fine tipoSesso -->
	<!-- tipoRichiesta -->
	<xs:simpleType name="tipoProvenienzaInfo">
		<xs:annotation>
			<xs:documentation>
				<p>Indica la fonte delle informazioni</p>
				<p>
					<b>VALORI AMMESSI</b>
					<ol>
						<li>1, da ANPR</li>
						<li>2, da INA</li>
						<li>3, da AT</li>
						<li>4, da AIRE centrale</li>
					</ol>
				</p>
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="numerico1"></xs:restriction>
	</xs:simpleType>
	<!-- Fine tipoRichiesta -->
	<!-- tipoInvioAmmesso -->
	<xs:simpleType name="tipoInvioAmmesso">
		<xs:restriction base="xs:string">
			<xs:minLength value="1" />
			<xs:maxLength value="20" />
		</xs:restriction>
	</xs:simpleType>
	<!-- Fine tipoInvioAmmesso -->
	<!-- tipoDatoStatoCivile -->
	<xs:simpleType name="tipoDatoStatoCivile">
		<xs:restriction base="numericoMax2">
		</xs:restriction>
	</xs:simpleType>
	<!-- Fine tipoDatoStatoCivile -->
	<!-- tipoStatoCF -->
	<xs:simpleType name="tipoStatoCF">
		<xs:restriction base="numericoMax2">
		</xs:restriction>
	</xs:simpleType>
	<!-- Fine tipoStatoCF -->
	<!-- tipoPosizioneProfessionale -->
	<xs:simpleType name="tipoPosizioneProfessionale">
		<xs:restriction base="charMax2">
		</xs:restriction>
	</xs:simpleType>
	<!-- Fine tipoPosizioneProfessionale -->
	<!-- tipoCondizioneNonProfessionale -->
	<xs:simpleType name="tipoCondizioneNonProfessionale">
		<xs:restriction base="numericoMax2">
		</xs:restriction>
	</xs:simpleType>
	<!-- Fine tipoCondizioneNonProfessionale -->
	<!-- tipoTitoloStudio -->
	<xs:simpleType name="tipoTitoloStudio">
		<xs:restriction base="charMax2">
		</xs:restriction>
	</xs:simpleType>
	<!-- Fine tipoTitoloStudio -->
	<!-- tipoSN -->
	<xs:simpleType name="tipoSN">
		<xs:restriction base="charMax20">
			<xs:pattern value="SI|NO|si|no|Si|No|s|n|S|N" />
		</xs:restriction>
	</xs:simpleType>
	<!-- Fine tipoSN -->
	<!-- tipoNumeroCivico -->
	<xs:simpleType name="tipoNumeroCiv">
		<xs:restriction base="numericoMax20"></xs:restriction>
	</xs:simpleType>
	<!-- Fine tipoNumeroCivico -->
	<!-- tipoColore -->
	<xs:simpleType name="tipoColore">
		<xs:restriction base="numerico1">
			<!-- <xs:pattern value="1|2"/> -->
		</xs:restriction>
	</xs:simpleType>
	<!-- Fine tipoColore -->
	<!-- tipoCartaceaElettronica -->
	<xs:simpleType name="tipoCartaceaElettronica">
		<xs:restriction base="numerico1">
		</xs:restriction>
	</xs:simpleType>
	<!-- Fine tipoCartaceaElettronica -->
	<!-- tipoDatoIndirizzo -->
	<xs:simpleType name="tipoDatoIndirizzo">
		<xs:restriction base="numericoMax2">
		</xs:restriction>
	</xs:simpleType>
	<!-- tipoSpecieFonte -->
	<xs:simpleType name="tipoSpecieFonte">
		<xs:restriction base="numerico1">
		</xs:restriction>
	</xs:simpleType>
	<!-- Fine tipoSpecieFonte -->
	<!-- tipoSpecieToponimo -->
	<xs:simpleType name="tipoSpecieToponimo">
		<xs:restriction base="numericoMax4">
		</xs:restriction>
	</xs:simpleType>
	<!-- Fine tipoSpecieToponimo -->
	<!-- tipoOperazioneRichiesta -->
	<xs:simpleType name="tipoOperazioneRichiesta">
		<xs:annotation>
			<xs:documentation>Codice del servizio così come riportati nella
				tabella servizi (es. 1001, 5005)
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="4" />
		</xs:restriction>
	</xs:simpleType>
	<!-- Fine tipoOperazioneRichiesta -->
	<!-- tipoRichiedenteOperazione -->
	<xs:simpleType name="tipoRichiedenteOperazione">
		<xs:restriction base="xs:string">
			<xs:length value="1" />
		</xs:restriction>
	</xs:simpleType>
	<!-- Fine tipoRichiedenteOperazione -->

	<!-- ##### tipi alfanumerici utili per interni scale piani degli indirizzi 
		###### -->
	<!-- alfanumericoParteIndirizzo4 massimo 4 caratteri -->
	<xs:simpleType name="alfanumericoParteIndirizzo4">
		<xs:restriction base="xs:string">
			<xs:minLength value="1" />
			<xs:maxLength value="4" />
			<xs:pattern value="([0-9À-ža-zA-Z\- &apos;/.,])*" />
		</xs:restriction>
	</xs:simpleType>
	<!-- Fine alfanumericoParteIndirizzo4 -->
	<!-- alfanumericoParteIndirizzo10 massimo 10 caratteri -->
	<xs:simpleType name="alfanumericoParteIndirizzo10">
		<xs:restriction base="xs:string">
			<xs:minLength value="1" />
			<xs:maxLength value="10" />
			<xs:pattern value="([0-9À-ža-zA-Z\- &apos;/.,])*" />
		</xs:restriction>
	</xs:simpleType>
	<!-- Fine alfanumericoParteIndirizzo10 -->
	<!-- ##### fine tipi alfanumerici utili per interni scale piani degli indirizzi 
		###### -->

	<!-- alfanumerico80char massimo 120 caratteri -->
	<xs:simpleType name="alfanumerico120char">
		<xs:restriction base="xs:string">
			<xs:minLength value="1" />
			<xs:maxLength value="120" />
			<xs:pattern value="([0-9À-ža-zA-Z\- &apos;/.,])*" />
		</xs:restriction>
	</xs:simpleType>
	<!-- Fine alfanumerico80char -->
	<!-- alfanumericoIndirizzo massimo 100 caratteri -->
	<xs:simpleType name="alfanumericoIndirizzo">
		<xs:restriction base="xs:string">
			<xs:minLength value="1" />
			<xs:maxLength value="100" />
			<xs:pattern value="([0-9À-ža-zA-Z\- &apos;/.,])*" />
