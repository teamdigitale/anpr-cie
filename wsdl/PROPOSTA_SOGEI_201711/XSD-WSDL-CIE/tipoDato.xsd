	<!-- tipoGeneralita -->
	<xs:complexType name="tipoGeneralita">
		<xs:annotation>
			<xs:documentation>Contiene le generalità di un soggetto
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="codiceFiscale" type="tipoCodiceFiscale"
				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						<p>Codice fiscale del soggetto</p>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice>
				<xs:element name="cognome" type="alfabeticoNomiCognomi">
					<xs:annotation>
						<xs:documentation>
							<p>Cognome del soggetto</p>
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="senzaCognome" type="tipoSenza">
					<xs:annotation>
						<xs:documentation>
							<p>Indica se soggetto è privo di cognome</p>
							<p>
								<b>REGOLE / CONDIZIONI</b>
								<ol>
									<li>Se presente deve valere 1 e deve essere presente il nome
									</li>
								</ol>
							</p>
						</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:choice>
				<xs:element name="nome" type="alfabeticoNomiCognomi">
					<xs:annotation>
						<xs:documentation>Il nome del soggetto</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="senzaNome" type="tipoSenza">
					<xs:annotation>
						<xs:documentation>
							<p>Indica se soggetto è privo di nome</p>
							<p>
								<b>REGOLE / CONDIZIONI</b>
								<ol>
									<li>Se presente deve valere 1 deve essere presente il cognome
									</li>
								</ol>
							</p>
						</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:element name="sesso" type="tipoSesso" minOccurs="0"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						<p>Il sesso del soggetto</p>
						<p>
							<b>VALORI AMMESSI</b>
							<ol>
								<li>F, femmina</li>
								<li>M, maschio</li>
								<li>N, non conosciuto</li>
							</ol>
						</p>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dataNascita" type="xs:date" minOccurs="0"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						<p>La data di nascita del soggetto</p>
						<p>
							<b>REGOLE / CONDIZIONI</b>
							<ol>
								<li>Se è completa, indicare AAAA-MM-GG</li>
								<li>Se è senza giorno, indicare AAAA-MM-01</li>
								<li>Se è senza mese e giorno, indicare AAAA-01-01</li>
							</ol>
						</p>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="senzaGiorno" type="tipoSenza" minOccurs="0"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						<p>
							<b>VALORI AMMESSI</b>
							<ol>
								<li>1, la data nascita è composta solo da mese e anno</li>
							</ol>
						</p>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="senzaGiornoMese" type="tipoSenza"
				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						<p>
							<b>VALORI AMMESSI</b>
							<ol>
								<li>1, la data nascita è composta dal solo anno</li>
							</ol>
						</p>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="luogoNascita" type="tipoLuogoEvento"
				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						<p>Luogo di nascita del soggetto</p>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="soggettoAIRE" type="char1" minOccurs="0"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						<p></p>
						<p>
							<b>REGOLE / CONDIZIONI</b>
							<ol>
								<li>Se S si richiede una interrogazione sui dati AIRE</li>
							</ol>
						</p>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="idSchedaSoggettoANPR" type="numericoMax15"
				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						<p>Identificativo della scheda soggetto attribuito da ANPR</p>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
